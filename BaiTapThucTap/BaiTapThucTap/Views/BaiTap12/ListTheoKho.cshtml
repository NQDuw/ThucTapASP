@model List<BaiTap11>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Số Phiếu xuất</th>
            <th>Ngày xuất Kho</th>
            <th>Kho</th>
            <th>Ghi Chú</th>
            <th> Action </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model)
        {
            <tr>
                <td>@(x.So_Phieu_Xuat_Kho ?? "N/A")</td>
                <td>@(x.Ngay_Xuat_Kho.ToShortDateString() ?? "N/A")</td>
                <td>@(x.Kho?.Ten_Kho ?? "N/A")</td>
                <td>@(x.Ghi_Chu ?? "N/A")</td>
                <td>
                    <a asp-action="Edit" asp-route-bai11Id="@x.Id" class="btn btn-danger"> Edit</a>
                    <a asp-controller="BaiTap9" asp-action="Index" asp-route-bai11Id="@x.Id" class="btn btn-warning">Chi Tiết </a>
                </td>
            </tr>
        }
    </tbody>
</table>